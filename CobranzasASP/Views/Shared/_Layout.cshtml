    /*--------------------sidebar-header----------------------*/

    .sidebar-wrapper .sidebar-header {
        padding: 10px;
        overflow: hidden;
    }

        .sidebar-wrapper .sidebar-header .user-pic {
            float: left;
            width: 60px;
            padding: 2px;
            border-radius: 12px;
            margin-right: 15px;
            overflow: hidden;
        }

            .sidebar-wrapper .sidebar-header .user-pic img {
                object-fit: cover;
                height: 100%;
                width: 100%;
            }

        .sidebar-wrapper .sidebar-header .user-info {
            float: left;
